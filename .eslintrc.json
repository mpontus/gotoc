{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended",
    "prettier"
  ],
  "plugins": [
    "flowtype"
  ],
  "rules": {
    "react/no-unused-prop-types": 0,
    "react/jsx-filename-extension": 0
  },
  "env": {
    "browser": true,
    "jest": true
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
